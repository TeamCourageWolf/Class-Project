<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:paddingLeft="10dip"
	android:paddingRight="10dip">
	<Button android:text="Hangout with Friends" android:id="@+id/plan_hangout_button" android:layout_width="match_parent" android:layout_height="wrap_content"></Button>
    <Button android:text="Study" android:id="@+id/plan_study_button" android:layout_height="wrap_content" android:layout_width="match_parent"></Button>    
    <Button android:text="Go to the Movies" android:id="@+id/plan_movie_button" android:layout_width="match_parent" android:layout_height="wrap_content"></Button>
    <Button android:text="Done" android:id="@+id/plan_done_button" android:layout_width="match_parent" android:layout_height="wrap_content"></Button>
</LinearLayout>